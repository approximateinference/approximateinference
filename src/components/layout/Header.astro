<script>
  function getCurrentTheme() {
    return localStorage.getItem("theme");
  }
  const toggleDarkModeButton = document.getElementById("toggleDarkMode");

  toggleDarkModeButton?.addEventListener("click", () => {
    const toggleDarkModeEvent = new CustomEvent("theme-change", {
      detail: { theme: getCurrentTheme() === "light" ? "dark" : "light" },
    });
    document.dispatchEvent(toggleDarkModeEvent);
  });
</script>

<script>
  // JavaScript to add 'active' class to the current page's link
  const currentUrl = window.location.pathname;

  // Get all links
  const links = document.querySelectorAll("nav a");
  links.forEach((link) => {
    if (link.getAttribute("href") === currentUrl && currentUrl !== "/") {
      link.classList.add("font-semibold"); // Add active styles to the current link
    }
  });
</script>
